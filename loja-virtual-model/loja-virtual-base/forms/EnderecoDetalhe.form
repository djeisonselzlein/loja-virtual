<?xml version="1.0" encoding="UTF-8"?>
<com.whitestein.lsps.modeler.form:FormDefinition
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:com.whitestein.lsps.modeler.form="http:///com/whitestein/lsps/modeler/form.ecore"
    xmi:id="_LjJeUANAEeO6n8ZCP11s6g"
    name="EnderecoDetalhe"
    newUI="true">
  <parameters
      xmi:id="_Vnc00ANAEeO6n8ZCP11s6g"
      name="endereco"
      parameterType="Endereco"/>
  <parameters
      xmi:id="_zc3iIAQAEeO4Z4YwFdbhJw"
      name="somenteLeitura"
      parameterType="Boolean"/>
  <formItem
      xsi:type="com.whitestein.lsps.modeler.form:NfdContainer"
      xmi:id="_LjJeUQNAEeO6n8ZCP11s6g"
      componentID="ROOT">
    <listeners
        xmi:id="__uFW8AQSEeO4Z4YwFdbhJw"
        name="VALIDATE"
        listenerType="ACTION_LISTENER"
        description="Validation listener">
      <validators
          xmi:id="__uFW8QQSEeO4Z4YwFdbhJw"
          placement="RUA">
        <message><![CDATA[if isEmpty(endereco.rua) then "Informe a Rua" end]]></message>
      </validators>
      <validators
          xmi:id="_WHA14gQVEeO4Z4YwFdbhJw"
          placement="NUMERO">
        <message><![CDATA[if isEmpty(endereco.numero) then "Informe Número" end]]></message>
      </validators>
      <validators
          xmi:id="_WHA14QQVEeO4Z4YwFdbhJw"
          placement="CIDADE">
        <message><![CDATA[if isEmpty(endereco.cidade) then "Informe Cidade" end]]></message>
      </validators>
      <validators
          xmi:id="_WHA14AQVEeO4Z4YwFdbhJw"
          placement="PAIS">
        <message><![CDATA[if isEmpty(endereco.pais) then "Informe Cidade" end]]></message>
      </validators>
    </listeners>
    <child
        xsi:type="com.whitestein.lsps.modeler.form:NfdPanel"
        xmi:id="_b6MlQANAEeO6n8ZCP11s6g">
      <child
          xsi:type="com.whitestein.lsps.modeler.form:NfdLayout"
          xmi:id="_ozYeMANAEeO6n8ZCP11s6g">
        <components
            xsi:type="com.whitestein.lsps.modeler.form:NfdTextBox"
            xmi:id="_p9ZaQANAEeO6n8ZCP11s6g"
            componentID="RUA">
          <hints
              xmi:id="_8jNqgAQfEeO4Z4YwFdbhJw"
              key="width"
              option="100%"/>
          <readonly>somenteLeitura</readonly>
          <binding><![CDATA[&endereco.rua]]></binding>
          <required>true</required>
          <label><![CDATA["Rua:"]]></label>
        </components>
        <components
            xsi:type="com.whitestein.lsps.modeler.form:NfdTextBox"
            xmi:id="_qxM1wANAEeO6n8ZCP11s6g"
            componentID="NUMERO">
          <hints
              xmi:id="_94I2EAQfEeO4Z4YwFdbhJw"
              key="width"
              option="100%"/>
          <readonly>somenteLeitura</readonly>
          <binding><![CDATA[&endereco.numero]]></binding>
          <required>true</required>
          <label><![CDATA["Número:"]]></label>
        </components>
        <components
            xsi:type="com.whitestein.lsps.modeler.form:NfdTextBox"
            xmi:id="_rUqvMANAEeO6n8ZCP11s6g"
            componentID="CIDADE">
          <hints
              xmi:id="_BCcO4AQgEeO4Z4YwFdbhJw"
              key="width"
              option="100%"/>
          <readonly>somenteLeitura</readonly>
          <binding><![CDATA[&endereco.cidade]]></binding>
          <required>true</required>
          <label><![CDATA["Cidade:"]]></label>
        </components>
        <components
            xsi:type="com.whitestein.lsps.modeler.form:NfdComboBox"
            xmi:id="_sUYfwANAEeO6n8ZCP11s6g"
            componentID="PAIS">
          <readonly>somenteLeitura</readonly>
          <binding><![CDATA[&endereco.pais]]></binding>
          <required>true</required>
          <label><![CDATA["País:"]]></label>
          <options>collect(getPaisesDoMundo(), {c -> new Option(value -> c, label -> c)})</options>
        </components>
      </child>
      <title><![CDATA["Endereço"]]></title>
    </child>
  </formItem>
  <description></description>
</com.whitestein.lsps.modeler.form:FormDefinition>
